<launch>
    <arg name="project" default="liovil_sam"/>
    
    <param name="scan_line" type="int" value="16" />

    <!-- if 1, do mapping 10 Hz, if 2, do mapping 5 Hz. Suggest to use 1, it will adjust frequence automaticlly -->
    <param name="mapping_skip_frame" type="int" value="1" />

    <!-- remove too closed points -->
    <param name="minimum_range" type="double" value="1.0"/>


    <param name="mapping_line_resolution" type="double" value="0.2"/>
    <param name="mapping_plane_resolution" type="double" value="0.4"/>

    <node pkg="$(arg project)" type="$(arg project)_ascanRegistration" name="ascanRegistration" />

    <node pkg="$(arg project)" type="$(arg project)_alaserOdometry" name="alaserOdometry"/>

    <node pkg="$(arg project)" type="$(arg project)_alaserMapping" name="alaserMapping" output="screen" />
    <!-- 
    -->

</launch>
